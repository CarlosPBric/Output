{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "sourceMap": true,
    "declaration": true,
    "declarationDir": "./lib",
    "strict": true,
    "strictNullChecks": false,
    "moduleResolution": "node",
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["./lib", "node_modules"]
}
